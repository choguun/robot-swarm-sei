# Example Environment Configuration
# Copy this file to .env and fill in your actual values

# =============================================================================
# WALLET CONFIGURATION - REQUIRED
# =============================================================================

# Your Sei wallet private key (get from MetaMask or wallet)
PRIVATE_KEY=abcdef1234567890abcdef1234567890abcdef1234567890abcdef1234567890

# =============================================================================
# NETWORK CONFIGURATION - PRE-CONFIGURED
# =============================================================================

# Sei Testnet (recommended for development)
SEI_TESTNET_RPC=https://evm-rpc-testnet.sei-apis.com
SEI_TESTNET_CHAIN_ID=1329

# =============================================================================
# OPTIONAL CONFIGURATION
# =============================================================================

# Seitrace API key (for contract verification)
SEI_ETHERSCAN_API_KEY=YOUR_API_KEY_HERE

# Demo mode (true for testing, false for live deployment)
DEMO_MODE=true

# Platform fee (2% default)
PLATFORM_FEE_BPS=200

# Auction duration (30 seconds for demo)
AUCTION_DURATION=30